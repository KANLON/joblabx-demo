<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>joblabxDemo</display-name>

	<servlet>
		<servlet-name>UserServlet</servlet-name>
		<servlet-class>com.kanlon.controller.UserServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>UserServlet</servlet-name>
		<url-pattern>/test</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>FileUploadServlet</servlet-name>
		<servlet-class>com.kanlon.controller.FileUploadServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>FileUploadServlet</servlet-name>
		<url-pattern>/uploadfile</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>SchoolExcelServlet</servlet-name>
		<servlet-class>com.kanlon.controller.SchoolExcelServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>SchoolExcelServlet</servlet-name>
		<url-pattern>/upload_school_file</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>FileUploadTempServlet</servlet-name>
		<servlet-class>com.kanlon.controller.FileUploadTempExcelServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>FileUploadTempServlet</servlet-name>
		<url-pattern>/uploadfile_temp</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>DataYearAndSchoolServlet</servlet-name>
		<servlet-class>com.kanlon.controller.DataYearAndSchoolServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>DataYearAndSchoolServlet</servlet-name>
		<url-pattern>/year_or_school</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>DataYearAndSchoolListServlet</servlet-name>
		<servlet-class>com.kanlon.controller.DataYearAndSchoolListServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>DataYearAndSchoolListServlet</servlet-name>
		<url-pattern>/year_or_school_list</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Top5NumSchoolServlet</servlet-name>
		<servlet-class>com.kanlon.controller.Top5NumSchoolServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Top5NumSchoolServlet</servlet-name>
		<url-pattern>/top5_school_list</url-pattern>
	</servlet-mapping>



	<servlet>
		<servlet-name>AllDataServlet</servlet-name>
		<servlet-class>com.kanlon.controller.AllDataServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AllDataServlet</servlet-name>
		<url-pattern>/all_data</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>DeleteAllDataServlet</servlet-name>
		<servlet-class>com.kanlon.controller.DeleteAllDataServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>DeleteAllDataServlet</servlet-name>
		<url-pattern>/delete_all_data</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>SexNumServlet</servlet-name>
		<servlet-class>com.kanlon.controller.SexNumServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>SexNumServlet</servlet-name>
		<url-pattern>/sexnum</url-pattern>
	</servlet-mapping>

	<error-page>
		<error-code>404</error-code>
		<location>/404.html</location>
	</error-page>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>